X = int(input('Insert first number : '))
Y = int(input('Insert seconf number : '))

arr = []

for i in range(X):
  tmp = []
  for j in range(Y):
    tmp.append(i*j)
  arr.append(tmp)
print(arr)

# 2nd way
x,y = map(int,input().split(','));
lst = [[i*j for j in range(y)] for i in range(x)]
print(lst)
